https://leetcode.com/problems/find-all-duplicates-in-an-array/
Code:

class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int> ans;
        for(int num:nums){
            int index=abs(num)-1;
            if(nums[index]<0) ans.push_back(index+1);
            nums[index]=-nums[index];
        }
        return ans;
    }
};
