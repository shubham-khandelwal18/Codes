https://www.interviewbit.com/problems/first-missing-integer/
Code:

int Solution::firstMissingPositive(vector<int> &A) {
    if(A.empty()) return 1;
    int n=A.size(),i=0;
    for(int j=0;j<n;j++)
    if(A[j]>0) A[i++]=A[j];
    if(i==0) return 1;
    for(int j=0;j<i;j++)
    if(abs(A[j])<=i)
        A[abs(A[j])-1]=A[abs(A[j])-1]<0?A[abs(A[j])-1]:-A[abs(A[j])-1];
    for(int j=0;j<i;j++)
    if(A[j]>0) return j+1;
    return i+1;
}
