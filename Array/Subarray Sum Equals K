https://leetcode.com/problems/subarray-sum-equals-k/
Code:

class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int ans=0,n=nums.size(),sum=0;
        map<int,int> m;
        m[0]=1;
        for(int i=0;i<n;i++){
            sum+=nums[i];
            ans+=m[sum-k];
            m[sum]++;
        }
        return ans;
    }
};
