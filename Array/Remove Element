https://leetcode.com/problems/remove-element/
Code:

class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        ios_base::sync_with_stdio(false);
        cin.tie(0);
        int i=0;
        for(int j=0;j<nums.size();j++){
            if(nums[j]!=val)
                nums[i++]=nums[j];
        }
        return i;
    }
};
